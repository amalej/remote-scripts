{
  "hosting": [
    {
      "target": "prod",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/node/setup",
          "destination": "/node/setup.bash"
        },
        {
          "source": "/remote-desktop/setup",
          "destination": "/remote-desktop/setup.bash"
        },
        {
          "source": "/vs-code/setup",
          "destination": "/vs-code/setup.bash"
        },
        {
          "source": "/firebase-tools/setup",
          "destination": "/fbase-tools/setup.bash"
        },
        {
          "source": "/testing/echo",
          "destination": "/testing/echo.bash"
        },
        {
          "source": "/new-test/echo",
          "destination": "/new-test/echo.bash"
        }
      ]
    }
  ]
}